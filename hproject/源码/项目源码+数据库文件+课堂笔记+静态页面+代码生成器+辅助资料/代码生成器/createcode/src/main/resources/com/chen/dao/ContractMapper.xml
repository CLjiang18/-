                    <result column="update_time" property="updateTime"/>
                    <result column="xgr" property="xgr"/>
                    <result column="rent_id" property="rentId"/>
                    <result column="payrent" property="payrent"/>
        </resultMap>

        <!-- 通用查询结果列 -->
        <sql id="Base_Column_List">
            id, customer_id, houses_id, qzrq, kfrq, tzrq, money, paymoney, create_time, djr, update_time, xgr, rent_id, payrent
